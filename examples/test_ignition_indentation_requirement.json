        "onActionPerformed": {
          "type": "script",
          "config": {
            "script": "import json\n\turl = 'http://localhost:8000'\n\tdata = {'test': 'value'}\nresponse = system.net.httpClient().post(url, data)\n\tprint('Done')"
          }
        }
